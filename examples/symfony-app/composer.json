{
    "name": "error-explorer/example-symfony-app",
    "description": "Symfony example application using Error Explorer Bundle",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../../core/php"
        },
        {
            "type": "path",
            "url": "../../frameworks/symfony"
        }
    ],
    "require": {
        "php": ">=8.1",
        "error-explorer/symfony": "@dev",
        "monolog/monolog": "^3.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/dotenv": "^6.4|^7.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "^6.4|^7.0",
        "symfony/twig-bundle": "7.0.*",
        "symfony/yaml": "^6.4|^7.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.50"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}
