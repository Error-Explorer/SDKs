{
    "name": "error-explorer/laravel-example",
    "description": "Example Laravel application demonstrating Error Explorer SDK",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "error-explorer/laravel": "@dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^11.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../frameworks/laravel"
        },
        {
            "type": "path",
            "url": "../../core/php"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
