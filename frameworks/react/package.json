{
  "name": "@error-explorer/react",
  "version": "1.0.0",
  "description": "Error Explorer SDK for React - Automatic error tracking with React integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@error-explorer/browser": "file:../../core/browser"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "happy-dom": "^12.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "error-explorer",
    "react",
    "error-tracking",
    "error-monitoring",
    "error-boundary",
    "breadcrumbs",
    "debugging"
  ],
  "author": "Error Explorer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Error-Explorer/SDKs.git",
    "directory": "frameworks/react"
  },
  "bugs": {
    "url": "https://github.com/Error-Explorer/SDKs/issues"
  },
  "homepage": "https://github.com/Error-Explorer/SDKs/tree/main/frameworks/react#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
