{
    "name": "error-explorer/laravel",
    "description": "Error Explorer SDK for Laravel - Automatic error tracking with breadcrumbs",
    "type": "library",
    "license": "MIT",
    "keywords": ["error", "tracking", "monitoring", "laravel", "exception", "breadcrumbs"],
    "authors": [
        {
            "name": "Error Explorer",
            "email": "support@error-explorer.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../core/php"
        }
    ],
    "require": {
        "php": "^8.1",
        "error-explorer/php-sdk": "@dev",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/log": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ErrorExplorer\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErrorExplorer\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ErrorExplorer\\Laravel\\ErrorExplorerServiceProvider"
            ],
            "aliases": {
                "ErrorExplorer": "ErrorExplorer\\Laravel\\Facades\\ErrorExplorer"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
